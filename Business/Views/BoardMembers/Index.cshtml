@{
  Layout = "_Layout";
}

@using Business.Models;
@model List<Business.Models.BoardMember>

<ul>
    @if (@Model.Count == 0)
    {
        <p>No board members found.</p>
    }
    @foreach (BoardMember whoever in @Model)
    {
        <li>Name: whoever.Name</li>
        <li>More Info: whoever.Description</li>
    }
</ul>


  <p>@Html.ActionLink("Back to charity details", "Details", "Charities")</p>